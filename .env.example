# Ambiente
NODE_ENV=development

# Configurações do Next.js
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Configurações do NestJS
API_PORT=3001
API_PREFIX=/api

# Configurações do Banco de Dados
DATABASE_URL="mongodb://localhost:27017/templatesia"
REDIS_URL="redis://localhost:6379"

# Autenticação
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=seu-segredo-aqui

# Provedores OAuth
GOOGLE_CLIENT_ID=seu-google-client-id
GOOGLE_CLIENT_SECRET=seu-google-client-secret

GITHUB_CLIENT_ID=seu-github-client-id
GITHUB_CLIENT_SECRET=seu-github-client-secret

# Serviços de Armazenamento
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Serviços de Email
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-username
SMTP_PASSWORD=your-smtp-password
EMAIL_FROM=noreply@templatesia.com

# Monitoramento e Logs
SENTRY_DSN=your-sentry-dsn
GRAYLOG_URL=http://localhost:9100
GRAYLOG_TOKEN=your-graylog-token

# Configurações de Cache
CACHE_TTL=3600

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Configurações de Segurança
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRATION=1d
COOKIE_SECRET=your-cookie-secret-key-here

# Configurações de Turborepo
TURBO_TOKEN=your-turbo-token
TURBO_TEAM=your-turbo-team

# MongoDB
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=changeme
MONGO_URI=mongodb://admin:changeme@localhost:27017/templatesia

# Redis
REDIS_PASSWORD=changeme
REDIS_URI=redis://:changeme@localhost:6379

# JWT
JWT_REFRESH_EXPIRATION=30d

# API
API_URL=http://localhost:3001
CORS_ORIGIN=http://localhost:3000

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:3001

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100

# Monitoring
SENTRY_DSN=your-sentry-dsn

# Logging
LOG_LEVEL=debug 