    "eslint": "^8.57.0",
    "@templatesia/eslint-config": "*",
    "@templatesia/tsconfig": "*",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}